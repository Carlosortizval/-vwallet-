CREATE DATABASE vwallet;

USE vwallet;

CREATE TABLE users (
    id INT AUTO_INCREMENT PRIMARY KEY,
    nombre VARCHAR(50),
    correo VARCHAR(50),
    password VARCHAR(100)
);

